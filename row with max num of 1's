#include<bits/stdc++.h>
using namespoace std;
class solution
{
public:
    int rowwithmax1's(vector<vector<int>> matrix,int r ,intc){
    int i=0;
    int j=c-1;
    int res=-1;
    while(i<r&&j<c){
        if(arr[i][j]==1){
            res=i;
            j--;
        }
        else{
            i++;
        }
        }
    return res;    
}; 
int main(){
    int r,c;
    cin>>r>>c;
    vector<vector<int>> matrix(r,vector<int>(c));
    for(int i=0;i<r;r++){
        for(int j=0;j<c;j++){
            cin>>arr[i][j];
        }
        }
    solution ob;
    auto ans=ob.rowwithmax1's(matrix,r,c);
    cout<<ans<<"\n";
    }
    return 0;
}    
